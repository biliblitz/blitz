{
  "name": "@biliblitz/blitz",
  "version": "0.1.5",
  "description": "A blitzing minimal backend-framework for Preact",
  "type": "module",
  "files": [
    "src",
    "dist",
    "env.d.ts",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biliblitz/blitz.git"
  },
  "types": "./dist/client/index.d.ts",
  "module": "./dist/client/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.mjs"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.mjs"
    },
    "./env": {
      "types": "./dist/env.d.ts"
    }
  },
  "scripts": {
    "fmt": "prettier --write src",
    "build": "rm -rf dist && tsc --build && cp src/env.d.ts dist && exbuild"
  },
  "license": "MIT",
  "peerDependencies": {
    "hono": "^4",
    "preact": "^10",
    "preact-render-to-string": "^6"
  },
  "devDependencies": {
    "@swwind/exbuild": "^1.0.8",
    "@types/node": "^20.14.11",
    "typescript": "^5.5.3",
    "vite": "^5.3.4"
  }
}
