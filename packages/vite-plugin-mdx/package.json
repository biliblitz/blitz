{
  "name": "@biliblitz/vite-plugin-mdx",
  "version": "0.0.1",
  "description": "A blitzing minimal backend-framework for Preact (Vite plugin for MDX)",
  "type": "module",
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biliblitz/blitz.git"
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "fmt": "prettier --write src",
    "build": "rm -rf dist && tsc --build && exbuild"
  },
  "license": "MIT",
  "devDependencies": {
    "@swwind/exbuild": "^1.0.8",
    "@types/node": "^20.14.11",
    "typescript": "^5.5.4",
    "vite": "^5"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.0.1",
    "vfile": "^6.0.2",
    "vfile-matter": "^5.0.0"
  }
}
