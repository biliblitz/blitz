{
  "name": "@biliblitz/adapter-static",
  "version": "0.0.1",
  "description": "A blitzing minimal backend-framework for Preact (Vite plugin for static target)",
  "type": "module",
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biliblitz/blitz.git"
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsc --build && exbuild"
  },
  "license": "MIT",
  "devDependencies": {
    "@biliblitz/blitz": "^0.2.0",
    "@swwind/exbuild": "^1.0.8",
    "@types/node": "^20.14.11",
    "typescript": "^5.5.4",
    "vite": "^5"
  },
  "dependencies": {
    "hono": "^4.2.3",
    "kolorist": "^1.8.0"
  }
}
