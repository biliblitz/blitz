{
  "name": "@biliblitz/vite",
  "version": "0.0.6",
  "description": "A blitzing minimal backend-framework for Preact (Vite plugins)",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/biliblitz/blitz.git"
  },
  "types": "./dist/types/index.d.ts",
  "module": "./dist/esm/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs"
    },
    "./adapters/static": {
      "types": "./dist/types/adapters/static.d.ts",
      "import": "./dist/esm/adapters/static.mjs"
    }
  },
  "scripts": {
    "fmt": "prettier --write src",
    "build": "rm -rf dist && tsc --build && node build.js"
  },
  "license": "MIT",
  "peerDependencies": {
    "vite": ">=5"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "esbuild": "^0.19.5",
    "tiny-glob": "^0.2.9",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@biliblitz/blitz": "^0.0.6",
    "@babel/core": "^7.23.9",
    "@babel/generator": "^7.23.6",
    "@babel/parser": "^7.23.9",
    "@babel/traverse": "^7.23.9",
    "@babel/types": "^7.23.9",
    "@hono/node-server": "^1.4.0",
    "@mdx-js/mdx": "^3.0.0",
    "chalk": "^5.3.0",
    "vfile": "^6.0.1",
    "vfile-matter": "^5.0.0"
  }
}
