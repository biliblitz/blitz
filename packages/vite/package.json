{
  "name": "@biliblitz/vite",
  "version": "0.2.0",
  "description": "A blitzing minimal backend-framework for Preact (Vite plugins)",
  "type": "module",
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biliblitz/blitz.git"
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./adapters/static": {
      "types": "./dist/adapters/static.d.ts",
      "import": "./dist/adapters/static.mjs"
    }
  },
  "scripts": {
    "fmt": "prettier --write src",
    "build": "rm -rf dist && tsc --build && exbuild"
  },
  "license": "MIT",
  "peerDependencies": {
    "hono": "^4",
    "vite": ">=5"
  },
  "devDependencies": {
    "@swwind/exbuild": "^1.0.8",
    "@types/node": "^20.14.11",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@biliblitz/blitz": "^0.2.0",
    "@hono/node-server": "^1.12.0",
    "@mdx-js/mdx": "^3.0.1",
    "@swc/core": "^1.6.13",
    "@swwind/remove-exports": "^2.1.3",
    "kolorist": "^1.8.0",
    "vfile": "^6.0.2",
    "vfile-matter": "^5.0.0"
  }
}
