{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "lib": ["ESNext", "DOM", "WebWorker"],
    "noEmit": true,
    "strict": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "jsxImportSource": "vue",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": [
      "node",
      "vite/client",
      "@biliblitz/blitz/env",
      "@cloudflare/workers-types/2023-07-01"
    ],
    "paths": {
      "~/*": ["./src/*"]
    }
  },
  "include": ["src", "adapters", "vite.config.ts"]
}
